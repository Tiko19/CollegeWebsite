@model IEnumerable<CollegeWebsite.Models.UserModel>

@{
    ViewBag.Title = "Users";
    Layout = "~/Views/Shared/_TeacherLayout.cshtml";
}

<h2>Users</h2>

<p>
    @Html.ActionLink("Create New", "UserCreate")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.UserID)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Password)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.UserID)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Password)
        </td>
        <td>
            @Html.ActionLink("Edit", "UserEdit", new { id=item.ID }) |
            @Html.ActionLink("Details", "UserDetails", new { id=item.ID }) |
            @Html.ActionLink("Delete", "UserDelete", new { id=item.ID })
        </td>
    </tr>
}

</table>

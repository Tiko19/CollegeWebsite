@model IEnumerable<CollegeWebsite.Models.AssociationsModel>

@{
    ViewBag.Title = "International Students Associations";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div>
    <h2>International Students Associations</h2>

    <table class="table">
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Association)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Description)
            </th>
            <th></th>
        </tr>

        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.Association)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Description)
                </td>
            </tr>
        }

    </table>

</div>